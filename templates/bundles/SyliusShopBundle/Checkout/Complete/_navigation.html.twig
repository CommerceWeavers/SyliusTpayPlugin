{% set payment = order.lastCartPayment() %}

{% if payment.method.gatewayConfig.gatewayName == 'tpay' %}
    {# >>> SyliusTpayPlugin customization #}
    {{ sylius_template_event('cw.tpay.shop.checkout.complete.navigation', { form, order, payment }) }}
